<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="fill_parent"
  android:layout_height="fill_parent" 
  android:orientation="vertical"
  android:background="@drawable/background">
    <TextView android:layout_height="wrap_content" android:id="@+id/textView1" android:text="Scores" android:layout_width="fill_parent" android:gravity="center"></TextView>
    <LinearLayout android:layout_height="75dip" android:orientation="horizontal" android:id="@+id/LinearLayout01" android:layout_weight="0" android:gravity="center" android:layout_width="fill_parent">
        <LinearLayout android:layout_height="wrap_content" android:orientation="vertical" android:id="@+id/LinearLayout03" android:layout_weight="1" android:gravity="center|top" android:layout_width="100dip">
            <TextView android:layout_height="wrap_content" android:gravity="center" android:id="@+id/TextView02" android:text="Component" android:layout_width="fill_parent"></TextView>
            <LinearLayout android:layout_height="fill_parent" android:id="@+id/LinearLayout05" android:layout_weight="1" android:gravity="center|left" android:layout_width="fill_parent">
                <TextView android:layout_height="wrap_content" android:gravity="center" android:id="@+id/textView3" android:text="MTC" android:layout_width="100dip"></TextView>
                <TextView android:layout_height="fill_parent" android:text="" android:gravity="center" android:id="@+id/cftActivityMoveToContactScoreLBL" android:layout_weight="0" android:layout_width="wrap_content"></TextView>
            </LinearLayout>
            <LinearLayout android:layout_height="fill_parent" android:id="@+id/LinearLayout06" android:layout_weight="1" android:gravity="center|left" android:layout_width="fill_parent">
                <TextView android:layout_height="wrap_content" android:gravity="center" android:id="@+id/textView4" android:text="ACL" android:layout_width="100dip"></TextView>
                <TextView android:layout_height="fill_parent" android:text="" android:gravity="center" android:id="@+id/cftActivityAmmoCanLiftsScoreLBL" android:layout_width="wrap_content" android:layout_weight="0"></TextView>
            </LinearLayout>
            <LinearLayout android:layout_height="fill_parent" android:id="@+id/linearLayout32" android:layout_weight="1" android:gravity="center|left" android:layout_width="fill_parent">
                <TextView android:layout_height="wrap_content" android:gravity="center" android:id="@+id/textView5" android:text="MUF" android:layout_width="100dip"></TextView>
                <TextView android:layout_height="fill_parent" android:text="" android:gravity="center" android:id="@+id/cftActivityManueverUnderFireScoreLBL" android:layout_width="wrap_content" android:layout_weight="0"></TextView>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout android:layout_height="fill_parent" android:orientation="vertical" android:id="@+id/LinearLayout04" android:layout_weight="1" android:gravity="center|top" android:layout_width="100dip">
            <TextView android:layout_height="wrap_content" android:id="@+id/textView6" android:text="Total" android:layout_width="wrap_content"></TextView>
            <TextView android:textSize="22dip" android:gravity="center" android:id="@+id/cftActivityTotalScoreLBL" android:text="" android:layout_width="130dip" android:layout_height="fill_parent"></TextView>
        </LinearLayout>
    </LinearLayout>
    <LinearLayout android:id="@+id/linearLayout9" android:layout_width="fill_parent" android:layout_weight="1" android:layout_height="15dip"></LinearLayout>
    <LinearLayout android:layout_height="wrap_content" android:orientation="vertical" android:id="@+id/linearLayout3" android:gravity="center" android:layout_width="fill_parent" android:layout_weight="1">
        <LinearLayout android:orientation="vertical" android:id="@+id/LinearLayout09" android:gravity="center" android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_weight="1">
            <LinearLayout android:layout_height="wrap_content" android:id="@+id/linearLayout11" android:layout_width="fill_parent">
                <LinearLayout android:layout_height="fill_parent" android:id="@+id/LinearLayout07" android:layout_weight="1" android:gravity="center" android:layout_width="1dip"></LinearLayout>
                <TextView android:layout_height="wrap_content" android:gravity="center" android:id="@+id/cftActivityMoveToContactTXTLBL" android:text="Move to Contact" android:layout_weight="0" android:layout_width="75dip"></TextView>
                <LinearLayout android:layout_height="fill_parent" android:id="@+id/linearLayout1" android:layout_weight="1" android:layout_width="1dip">
                    <TextView android:layout_height="fill_parent" android:id="@+id/cftActivityMoveToContactLBL" android:text="00:00" android:layout_weight="0" android:layout_width="75dip" android:gravity="center|center_vertical"></TextView>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearLayout20" android:gravity="center" android:layout_width="fill_parent" android:layout_height="wrap_content">
                <LinearLayout android:layout_height="fill_parent" android:id="@+id/linearLayout31" android:gravity="center" android:layout_width="60dip">
                    <Button android:layout_height="25dip" android:text="" android:id="@+id/cftActivityMoveToContactDownBTN" android:background="@drawable/minus_button" android:layout_weight="0" android:layout_width="50dip"></Button>
                </LinearLayout>
                <SeekBar android:layout_height="wrap_content" android:layout_weight="1" android:id="@+id/cftActivityMoveToContactSeekBar" android:layout_width="fill_parent"></SeekBar>
                <LinearLayout android:layout_height="fill_parent" android:id="@+id/linearLayout30" android:gravity="center" android:layout_width="60dip">
                    <Button android:layout_height="25dip" android:text="" android:id="@+id/cftActivityMoveToContactUpBTN" android:background="@drawable/plus_button" android:layout_weight="0" android:layout_width="50dip"></Button>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout android:orientation="vertical" android:id="@+id/linearLayout6" android:gravity="center" android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_weight="1">
            <LinearLayout android:layout_height="wrap_content" android:id="@+id/linearLayout8" android:layout_width="fill_parent">
                <LinearLayout android:layout_height="fill_parent" android:id="@+id/linearLayout15" android:layout_weight="1" android:gravity="center" android:layout_width="1dip"></LinearLayout>
                <TextView android:layout_height="wrap_content" android:gravity="center" android:id="@+id/textView7" android:text="Ammo Can Lifts" android:layout_width="75dip" android:layout_weight="0"></TextView>
                <LinearLayout android:layout_height="fill_parent" android:id="@+id/linearLayout2" android:layout_weight="1" android:layout_width="1dip">
                    <TextView android:layout_height="fill_parent" android:id="@+id/cftActivityAmmoCanLiftsNumLBL" android:text="0" android:layout_weight="0" android:layout_width="75dip" android:gravity="center|center_vertical"></TextView>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearLayout19" android:gravity="center" android:layout_width="fill_parent" android:layout_height="wrap_content">
                <LinearLayout android:layout_height="fill_parent" android:id="@+id/linearLayout29" android:gravity="center" android:layout_width="60dip">
                    <Button android:layout_height="25dip" android:id="@+id/cftActivityAmmoCanLiftsDownBTN" android:background="@drawable/minus_button" android:layout_weight="0" android:text="" android:layout_width="50dip"></Button>
                </LinearLayout>
                <SeekBar android:layout_height="wrap_content" android:layout_weight="1" android:id="@+id/cftActivityAmmoCanLiftsSeekBar" android:layout_width="fill_parent"></SeekBar>
                <LinearLayout android:layout_height="fill_parent" android:id="@+id/linearLayout28" android:gravity="center" android:layout_width="60dip">
                    <Button android:layout_height="25dip" android:id="@+id/cftActivityAmmoCanLiftsUpBTN" android:background="@drawable/plus_button" android:layout_weight="0" android:text="" android:layout_width="50dip"></Button>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout android:orientation="vertical" android:id="@+id/LinearLayout02" android:gravity="center" android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_weight="1">
            <LinearLayout android:layout_height="wrap_content" android:id="@+id/linearLayout7" android:layout_width="fill_parent">
                <LinearLayout android:layout_height="fill_parent" android:id="@+id/linearLayout16" android:layout_weight="1" android:gravity="center" android:layout_width="1dip"></LinearLayout>
                <TextView android:layout_height="wrap_content" android:gravity="center" android:id="@+id/TextView01" android:text="Manuever Under Fire" android:layout_width="75dip" android:layout_weight="0"></TextView>
                <LinearLayout android:layout_height="fill_parent" android:id="@+id/linearLayout5" android:layout_weight="1" android:layout_width="1dip">
                    <TextView android:layout_height="fill_parent" android:id="@+id/cftActivityManueverUnderFireLBL" android:text="00:00" android:layout_weight="0" android:layout_width="75dip" android:gravity="center|center_vertical"></TextView>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearLayout17" android:gravity="center" android:layout_width="fill_parent" android:layout_height="wrap_content">
                <LinearLayout android:layout_height="fill_parent" android:id="@+id/linearLayout27" android:gravity="center" android:layout_width="60dip">
                    <Button android:layout_height="25dip" android:id="@+id/cftActivityManueverUnderFireDownBTN" android:background="@drawable/minus_button" android:layout_weight="0" android:text="" android:layout_width="50dip"></Button>
                </LinearLayout>
                <SeekBar android:layout_height="wrap_content" android:layout_weight="1" android:id="@+id/cftActivityManueverUnderFireSeekBar" android:layout_width="fill_parent"></SeekBar>
                <LinearLayout android:layout_height="fill_parent" android:id="@+id/linearLayout25" android:gravity="center" android:layout_width="60dip">
                    <Button android:layout_height="25dip" android:id="@+id/cftActivityManueverUnderFireUpBTN" android:background="@drawable/plus_button" android:layout_weight="0" android:text="" android:layout_width="50dip"></Button>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
    <LinearLayout android:id="@+id/linearLayout10" android:layout_width="fill_parent" android:layout_weight="1" android:layout_height="15dip"></LinearLayout>
    <LinearLayout android:orientation="vertical" android:id="@+id/linearLayout24" android:layout_width="fill_parent" android:layout_height="wrap_content">
        <LinearLayout android:layout_height="wrap_content" android:id="@+id/linearLayout4" android:layout_weight="1" android:layout_width="fill_parent">
            <LinearLayout android:layout_height="fill_parent" android:id="@+id/linearLayout14" android:layout_weight="1" android:gravity="center" android:layout_width="50dip">
                <RadioGroup android:id="@+id/RadioGroup01" android:layout_height="40dip" android:orientation="horizontal" android:layout_width="150dip">
                    <com.andrios.marinepft.SegmentedControlButton android:textSize="12dip" android:layout_height="fill_parent" android:layout_weight="1" android:id="@+id/cftActivityMaleSegment" android:layout_width="50dip" android:checked="true" android:text="Male"></com.andrios.marinepft.SegmentedControlButton>
                    <com.andrios.marinepft.SegmentedControlButton android:textSize="12dip" android:layout_height="fill_parent" android:layout_weight="1" android:id="@+id/cftActivityFemaleSegment" android:layout_width="50dip" android:checked="false" android:text="Female"></com.andrios.marinepft.SegmentedControlButton>
                </RadioGroup>
            </LinearLayout>
            <LinearLayout android:layout_height="fill_parent" android:id="@+id/linearLayout21" android:layout_weight="1" android:gravity="center" android:layout_width="50dip">
                <LinearLayout android:layout_height="fill_parent" android:id="@+id/linearLayout23" android:gravity="center" android:layout_width="wrap_content">
                    <RadioGroup android:id="@+id/group1" android:layout_height="40dip" android:orientation="horizontal" android:layout_width="150dip">
                        <com.andrios.marinepft.SegmentedControlButton android:textSize="10dip" android:layout_height="fill_parent" android:layout_weight="1" android:id="@+id/cftActivityAge17Segment" android:layout_width="25dip" android:checked="true" android:text="17-26"></com.andrios.marinepft.SegmentedControlButton>
                        <com.andrios.marinepft.SegmentedControlButton android:textSize="10dip" android:layout_height="fill_parent" android:layout_weight="1" android:id="@+id/cftActivityAge27Segment" android:layout_width="25dip" android:checked="false" android:text="27-39"></com.andrios.marinepft.SegmentedControlButton>
                        <com.andrios.marinepft.SegmentedControlButton android:textSize="10dip" android:layout_height="fill_parent" android:layout_weight="1" android:id="@+id/cftActivityAge40Segment" android:layout_width="25dip" android:checked="false" android:text="40-45"></com.andrios.marinepft.SegmentedControlButton>
                        <com.andrios.marinepft.SegmentedControlButton android:textSize="10dip" android:layout_height="fill_parent" android:layout_weight="1" android:id="@+id/cftActivityAge46Segment" android:layout_width="25dip" android:checked="false" android:text="46+"></com.andrios.marinepft.SegmentedControlButton>
                    </RadioGroup>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
  
    
    <RelativeLayout android:layout_width="fill_parent" 
      android:layout_height="50dip" 
      android:layout_weight="0" 
      android:id="@+id/cftActivityBottomBar">
        <Button 
        	android:id="@+id/cftActivityLogBTN" 
        	android:layout_height="wrap_content"
        	android:layout_centerInParent="true" 
        	android:visibility="invisible"
        	android:text="Log CFT" android:layout_width="125dip">
        </Button>
  	<com.google.ads.AdView 
		xmlns:ads="http://schemas.android.com/apk/lib/com.google.ads"
		android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        ads:adUnitId="a14deec86da5f51"
        ads:adSize="BANNER"
        android:layout_weight="0" android:id="@+id/CFTAdView"/>			
    </RelativeLayout>
</LinearLayout>
